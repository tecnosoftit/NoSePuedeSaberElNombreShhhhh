<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified" attributeFormDefault="unqualified" version="1.0.0">
	<xs:include schemaLocation="HotelbedsSimpleTypes.xsd"/>
	<xs:attributeGroup name="AgeRange">
		<xs:annotation>
			<xs:documentation>Attribute group to define an age range.</xs:documentation>
		</xs:annotation>
		<xs:attribute name="ageFrom" use="optional">
			<xs:annotation>
				<xs:documentation>Range age from.</xs:documentation>
			</xs:annotation>
			<xs:simpleType>
				<xs:restriction base="xs:int">
					<xs:minInclusive value="0"/>
					<xs:maxInclusive value="999"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
		<xs:attribute name="ageTo" use="optional">
			<xs:annotation>
				<xs:documentation>Range age to.</xs:documentation>
			</xs:annotation>
			<xs:simpleType>
				<xs:restriction base="xs:int">
					<xs:minInclusive value="0"/>
					<xs:maxInclusive value="999"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
	</xs:attributeGroup>
	<xs:complexType name="AdditionalCost">
		<xs:annotation>
			<xs:documentation>Service additional cost.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Price" type="Price" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Additional cost price list.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
		<xs:attribute name="type">
			<xs:annotation>
				<xs:documentation>Type of the additional cost.</xs:documentation>
			</xs:annotation>
			<xs:simpleType>
				<xs:restriction base="xs:string">
					<xs:minLength value="1"/>
					<xs:maxLength value="15"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
	</xs:complexType>
	<xs:complexType name="AdditionalCostList">
		<xs:annotation>
			<xs:documentation>List of additional costs.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="AdditionalCost" type="AdditionalCost" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Additional cost.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="Address">
		<xs:annotation>
			<xs:documentation>Address data.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="StreetTypeId" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Street type identifier.</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:minLength value="1"/>
						<xs:maxLength value="2"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="StreetTypeName" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Street type name.</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:minLength value="1"/>
						<xs:maxLength value="40"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="StreetName" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Street name.</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:minLength value="1"/>
						<xs:maxLength value="50"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="Number" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Number for this address.</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:minLength value="1"/>
						<xs:maxLength value="10"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="PostalCode" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Postal code.</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:minLength value="1"/>
						<xs:maxLength value="10"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="City" minOccurs="0">
				<xs:annotation>
					<xs:documentation>City name.</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:minLength value="1"/>
						<xs:maxLength value="50"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="State" minOccurs="0">
				<xs:annotation>
					<xs:documentation>State or province name.</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:minLength value="1"/>
						<xs:maxLength value="40"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="CountryCode" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Country code.</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:minLength value="1"/>
						<xs:maxLength value="3"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="AgencyIdentification">
		<xs:annotation>
			<xs:documentation>Data to identify an agency.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Code">
				<xs:annotation>
					<xs:documentation>Agency code.</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:int">
						<xs:minInclusive value="0"/>
						<xs:maxInclusive value="99999999"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="Branch">
				<xs:annotation>
					<xs:documentation>Agency branch.</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:int">
						<xs:minInclusive value="0"/>
						<xs:maxInclusive value="99999999"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="AuditData">
		<xs:annotation>
			<xs:documentation>Audit data returned in all responses.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="ProcessTime" type="xs:long">
				<xs:annotation>
					<xs:documentation>System processing time in miliseconds.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Timestamp">
				<xs:annotation>
					<xs:documentation>Response timestamp.</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:minLength value="1"/>
						<xs:maxLength value="23"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="RequestHost">
				<xs:annotation>
					<xs:documentation>Requesting host system ip.</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:minLength value="1"/>
						<xs:maxLength value="200"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="ServerName">
				<xs:annotation>
					<xs:documentation>Server name that processed the request.</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:minLength value="1"/>
						<xs:maxLength value="50"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="ServerId">
				<xs:annotation>
					<xs:documentation>Server id that processed the request.</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:minLength value="1"/>
						<xs:maxLength value="2"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="SchemaRelease">
				<xs:annotation>
					<xs:documentation>XML schema release used to process the request in the system.</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:minLength value="1"/>
						<xs:maxLength value="10"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="HydraCoreRelease">
				<xs:annotation>
					<xs:documentation>Internal component release information.</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:minLength value="1"/>
						<xs:maxLength value="25"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="HydraEnumerationsRelease">
				<xs:annotation>
					<xs:documentation>Internal component release information.</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:minLength value="1"/>
						<xs:maxLength value="25"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="MerlinRelease">
				<xs:annotation>
					<xs:documentation>Internal component release information.</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:minLength value="1"/>
						<xs:maxLength value="25"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="CancellationProtection">
		<xs:annotation>
			<xs:documentation>Data to represent a cancel proctection element.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Status" type="HotelbedsServiceStatus">
				<xs:annotation>
					<xs:documentation>Indicates whether the cancellation protection is new in the purchase 'NEW', has been confirmed 'CONFIRMED', or has been cancelled 'CANCELLED'.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Price" type="Price" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The price of the cancel protection.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ModificationPolicyList" type="ServiceModificationPolicyList" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The mdification policy for the cancel protection.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="CancellationPolicy" type="PriceList" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Cancellation protection cancel policy.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="CommentList" type="CommentList" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Comment list for the cancellation protection.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="AdditionalCostList" type="AdditionalCostList" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Additional costs list for the cancellation protection.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ErrorList" type="HotelbedsErrorList" minOccurs="0">
				<xs:annotation>
					<xs:documentation>List of errors for the cancellation protection.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="Classification">
		<xs:annotation>
			<xs:documentation>Generic classification data.</xs:documentation>
		</xs:annotation>
		<xs:simpleContent>
			<xs:extension base="StringLength0to50">
				<xs:attribute name="code" use="required">
					<xs:annotation>
						<xs:documentation>Classification code.</xs:documentation>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:minLength value="1"/>
							<xs:maxLength value="5"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:attribute>
			</xs:extension>
		</xs:simpleContent>
	</xs:complexType>
	<xs:complexType name="Comment">
		<xs:annotation>
			<xs:documentation>Comment data.</xs:documentation>
		</xs:annotation>
		<xs:simpleContent>
			<xs:extension base="StringLength1to2000">
				<xs:attribute name="type" type="CommentType">
					<xs:annotation>
						<xs:documentation>Type of the comment.</xs:documentation>
					</xs:annotation>
				</xs:attribute>
			</xs:extension>
		</xs:simpleContent>
	</xs:complexType>
	<xs:complexType name="CommentList">
		<xs:annotation>
			<xs:documentation>Comment list.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Comment" type="Comment" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="ConfirmationPurchaseData">
		<xs:annotation>
			<xs:documentation>Needed data to confirm a Purchase.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Holder" type="Customer" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Holder data for the purchase. Mandatory if the Purchase to confirm has the SHOPPING_CART status.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="AgencyReference" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Agency reference code to identify the purchase in the remote system. Mandatory if the Purchase to confirm has the SHOPPING_CART status.</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:minLength value="1"/>
						<xs:maxLength value="20"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="ConfirmationServiceDataList" type="ConfirmationServiceDataList" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Additional data to confirm a Service.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="CommentList" type="CommentList" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Comment list.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
		<xs:attribute name="purchaseToken" use="required">
			<xs:annotation>
				<xs:documentation>Internal token needed to confirm the purchase.</xs:documentation>
			</xs:annotation>
			<xs:simpleType>
				<xs:restriction base="xs:string">
					<xs:minLength value="1"/>
					<xs:maxLength value="11"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
	</xs:complexType>
	<xs:complexType name="ConfirmationServiceData" abstract="true">
		<xs:annotation>
			<xs:documentation>Needed data to confirm a Service.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="CustomerList" type="CustomerList">
				<xs:annotation>
					<xs:documentation>Customer list for this service.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="CommentList" type="CommentList" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Comment list for the service.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
		<xs:attribute name="SPUI" use="required">
			<xs:annotation>
				<xs:documentation>(Service Purchase Unique Identifier). Unique identifier of the service.</xs:documentation>
			</xs:annotation>
			<xs:simpleType>
				<xs:restriction base="xs:string">
					<xs:minLength value="1"/>
					<xs:maxLength value="30"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
	</xs:complexType>
	<xs:complexType name="ConfirmationServiceDataCar">
		<xs:annotation>
			<xs:documentation>Needed data to confirm a Car service.</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="ConfirmationServiceData"/>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="ConfirmationServiceDataHotel">
		<xs:annotation>
			<xs:documentation>Needed data to confirm a Hotel service.</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="ConfirmationServiceData"/>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="ConfirmationServiceDataInsurance">
		<xs:annotation>
			<xs:documentation>Needed data to confirm an Insurance service.</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="ConfirmationServiceData"/>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="ConfirmationServiceDataList">
		<xs:annotation>
			<xs:documentation>Confirmation service data list.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="ServiceData" type="ConfirmationServiceData" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="ConfirmationServiceDataTicket">
		<xs:annotation>
			<xs:documentation>Needed data to confirm a Ticket service.</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="ConfirmationServiceData">
				<xs:sequence>
					<xs:element name="ServiceDetailList" type="ServiceDetailList" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Details about ticket service.</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="ConfirmationServiceDataTransfer">
		<xs:annotation>
			<xs:documentation>Needed data to confirm a Transfer service.</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="ConfirmationServiceData">
				<xs:sequence>
					<xs:element name="TravelInfo" type="ServiceTransferTravelInfo" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Travel info for the transfer.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="HotelName" type="StringLength1to50" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Hotel Name.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="HotelAddress" type="StringLength1to50" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Hotel Address.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="HotelPostalCode" type="StringLength1to10" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Hotel Postal Code.</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="ContactInfo">
		<xs:annotation>
			<xs:documentation>Contact information.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Address" type="Address" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Address information.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="EmailList" type="ProductEmailList" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Email list.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="PhoneList" type="ProductContactNumberList" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Phone list.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="FaxList" type="ProductContactNumberList" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Fax list.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="WebList" type="ProductWebList" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Web list.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="Contract">
		<xs:annotation>
			<xs:documentation>Contract information.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Name">
				<xs:annotation>
					<xs:documentation>Contract name.</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:minLength value="1"/>
						<xs:maxLength value="20"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="IncomingOffice" type="IncomingOffice">
				<xs:annotation>
					<xs:documentation>Contract incoming office.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Sequence" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Contract sequence.</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:minLength value="1"/>
						<xs:maxLength value="200"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="Classification" type="ContractClassification" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Contract classification.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="CommentList" type="CommentList" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Contract comments.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="ContractClassification">
		<xs:annotation>
			<xs:documentation>Contract classification information.</xs:documentation>
		</xs:annotation>
		<xs:simpleContent>
			<xs:extension base="StringLength0to50">
				<xs:attribute name="code" use="required">
					<xs:annotation>
						<xs:documentation>Contract classification code.</xs:documentation>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:minLength value="1"/>
							<xs:maxLength value="5"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:attribute>
			</xs:extension>
		</xs:simpleContent>
	</xs:complexType>
	<xs:complexType name="ContractClassificationList">
		<xs:annotation>
			<xs:documentation>Contract classification list.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="ContractClassification" type="ContractClassification" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="ContractList">
		<xs:annotation>
			<xs:documentation>Contract list.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Contract" type="Contract" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="CoreRequest" abstract="true">
		<xs:annotation>
			<xs:documentation>Common request data.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Language" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Desired response language.</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:minLength value="1"/>
						<xs:maxLength value="3"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
